{
  "version": 2,
  "buildCommand": "yarn build",
  "installCommand": "yarn install",
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node",
      "includeFiles": "dist/**",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ]
}